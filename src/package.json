{
  "name": "template",
  "version": "0.1.0",
  "private": true,
  "main": "src/electron-starter.js",
  "dependencies": {
    "babel": "^6.23.0",
    "connected-react-router": "^6.4.0",
    "electron": "^5.0.1",
    "electron-packager": "^13.1.1",
    "expo": "^32.0.6",
    "react": "^16.8.6",
    "react-art": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-file-reader": "^1.1.4",
    "react-hot-loader": "^4.8.5",
    "react-native-cli": "^2.0.1",
    "react-native-gesture-handler": "^1.2.1",
    "react-native-web": "^0.11.2",
    "react-navigation": "^3.10.2",
    "react-redux": "^7.0.3",
    "react-router": "^5.0.0",
    "react-router-config": "^5.0.0",
    "react-router-dom": "^5.0.0",
    "react-router-native": "^5.0.0",
    "react-scripts": "3.0.0"
  },
  "scripts": {
    "first": "mv babelrc .babelrc && npm install && sleep 50 && npm run electron-packager && open ./template-darwin-x64/template.app",
    "start": "node src/server/server.js",
    "build": "react-scripts build && react-snapshot",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "start-web": "react-scripts start",
    "build-web": "react-scripts build",
    "test-web": "react-scripts test",
    "eject-web": "react-scripts eject",
    "start-native": "expo start",
    "android": "expo android",
    "ios": "expo ios",
    "build:ios": "expo build:ios",
    "build:android": "expo build:android",
    "electron": "electron ./src/electron-starter.js",
    "electron-dev": "electron ./src/dev-electron-starter.js",
    "react-start": "BROWSER=none react-scripts start",
    "dev": "npm run react-start & node ./src/electron-wait-react.js",
    "react-build": "react-scripts build",
    "preelectron-pack": "npm run build",
    "electron-packager": "npm run preelectron-pack & electron-packager . --productName=template --icon=./icons/icon.icns --overwrite && open ./template-darwin-x64/template.app",
    "postinstall": "echo finised"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-react-jsx-source": "^6.22.0",
    "babel-preset-expo": "^5.1.1",
    "redux-devtools-extension": "^2.13.8"
  },
  "homepage": "./",
  "build": {
    "appId": "nonExistentID",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "public"
    }
  },
  "_moduleAliases": {
    "react-native":"react-native-web"
  }
}
